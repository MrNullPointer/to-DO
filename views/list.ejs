<%- include("header") -%> 
    <div class="box" id="heading">
      <h1><%=listTitle%></h1>
    </div>

    <div class="box">
      <%for(var item = 0; item < newListItems.length; item++){%>
      <div class="item">
        <input type="checkbox" />
        <p><%=newListItems[item]%></p>
      </div>
      <%}%>

      <form class="item" action="/" method="POST">
        <input
          type="text"
          name="newItem"
          placeholder="New Item"
          autocomplete="off"
        />
        <button type="submit" name="list" value=<%=listTitle %>> +</button>
      </form>
    </div>
 
<%- include("footer") -%>